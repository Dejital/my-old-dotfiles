###
# Various Aliases
###

# Mvim
alias v='mvim'
alias vf='mvim -f'

# Terminal
alias l='ls -G'
alias ll='ls -aG'
alias ..='cd ..'
alias ...='cd ...'
alias school='cd ~/Documents/School'

# Latex
alias lpdf='pdflatex'
alias lrtf='latex2rtf'
# Word is for opening RTF files quickly in Microsoft Word
alias word='open -a "Microsoft Word"'

# Python
alias p='python'
alias p3='python3'

# Git
alias gs='git status '
alias ga='git add '
alias gl='git log --oneline'
alias gln='git log --oneline --name-only'
alias glg='git log --oneline --graph --all --decorate'
alias gb='git branch '
alias gc='git commit'
alias gd='git diff --color-words'
alias go='git checkout '
alias gk='gitk --all&'
alias gx='gitx --all'
alias gh='git hist'


